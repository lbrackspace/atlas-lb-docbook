    &lt;message&gt;Validation fault&lt;/message&gt;
    &lt;details&gt;The object is not valid&lt;/details&gt;
        &lt;validationErrors&gt;
            &lt;message&gt;Node ip is invalid. Please specify a valid ip.&lt;/message&gt;
        &lt;/validationErrors&gt;
&lt;/badRequest&gt;
            </fo:block></fo:block><fo:block id="d0e2294"><fo:block xmlns:d="http://docbook.org/ns/docbook"><fo:block><fo:block keep-together.within-column="always" margin-left="0pt" font-family="CartoGothic Std"><fo:block keep-with-next.within-column="always"><fo:block font-family="CartoGothic Std" font-weight="bold" keep-with-next.within-column="always" space-before.minimum="0.8em" space-before.optimum="1.0em" space-before.maximum="1.2em" text-align="start" start-indent="0pt" color="rgb(196,0,34)"><fo:marker marker-class-name="section.head.marker">itemNotFound</fo:marker><fo:block font-size="20.735999999999997pt">5.4. itemNotFound</fo:block></fo:block></fo:block></fo:block></fo:block></fo:block><fo:block space-before.optimum="1em" space-before.minimum="0.8em" space-before.maximum="1.2em">
