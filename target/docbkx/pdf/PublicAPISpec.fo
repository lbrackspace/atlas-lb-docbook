                            <fo:table-cell padding-start="2pt" padding-end="2pt" padding-top="2pt" padding-bottom="2pt" border-start-style="none" border-top-style="none" border-end-style="solid" border-end-width="0.5pt" border-end-color="black"><fo:block>/loadbalancers/algorithms</fo:block></fo:table-cell>
                            <fo:table-cell padding-start="2pt" padding-end="2pt" padding-top="2pt" padding-bottom="2pt" border-start-style="none" border-top-style="none"><fo:block>List all supported load balancing algorithms</fo:block></fo:table-cell>
